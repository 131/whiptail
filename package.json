{
  "name": "whiptail",
  "version": "1.6.2",
  "description": "Whiptail wrapper for nodejs",
  "main": "index.js",
  "scripts": {
    "test": "npm run cover && npm run eslint",
    "eslint": "eslint index.js",
    "mocha": "mocha -b",
    "cover": "nyc --temp-directory coverage/.nyc_output --report-dir coverage/master --silent --reporter none node_modules/mocha/bin/_mocha -b test/ && nyc --temp-directory coverage/.nyc_output  report --include \"coverage/**/coverage*.json\" --report-dir coverage/html/ --reporter text-summary --reporter html --reporter lcov",
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/html/lcov.info"
  },
  "author": "Francois Leurent <131.js@cloudyks.org>",
  "license": "ISC",
  "dependencies": {
    "nyks": "^5.11.1"
  },
  "devDependencies": {
    "eslint-plugin-ivs": "^2.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.1.2",
    "nyc": "^13.0.1",
    "pty.js": "^0.3.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/131/whiptail.git"
  },
  "keywords": [
    "whiptail",
    "menu",
    "terminal"
  ],
  "bugs": {
    "url": "https://github.com/131/whiptail/issues"
  },
  "homepage": "https://github.com/131/whiptail#readme"
}
